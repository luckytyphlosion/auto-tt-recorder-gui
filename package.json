{
  "name": "auto-tt-recorder-gui",
  "productName": "Auto-TT-Recorder GUI",
  "description": "GUI wrapper for Auto-TT-Recorder",
  "author": "luckytyphlosion",
  "version": "1.1.2",
  "autoTTRecorderVersion": "1.3.8",
  "dolphinVersion": "1.0.1",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@hookform/error-message": "^2.0.1",
    "@myrpc/documents-folder": "^1.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@uiw/react-codemirror": "^4.21.7",
    "async-mutex": "^0.4.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "electron-context-menu": "^3.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-modal": "^3.16.1",
    "react-refresh": "^0.11.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1",
    "yaml": "^2.1.1"
  },
  "main": "dist/main/main.js",
  "scripts": {
    "build": "npm-run-all build-main build-renderer",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack/main.webpack.ts --mode=production",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack/renderer.webpack.ts --mode=production",
    "start-main": "cross-env NODE_ENV=development electronmon -r ts-node/register ./src/main/electron.ts",
    "start-renderer": "cross-env NODE_ENV=development webpack serve --config webpack/renderer.webpack.ts --mode=development",
    "dev": "concurrently -k \"cross-env BROWSER=none npm run start-renderer\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && npm run start-main",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "pack": "node build_scripts/pack.js",
    "installer2": "electron-builder --win --x64",
    "build-installer2": "npm-run-all build installer2"
  },
  "electronmon": {
    "patterns": [
      "!auto-tt-recorder_*_for_gui/**/*",
      "!auto-tt-recorder-gui-v*-win32-x64/**/*",
      "!dist/**/*",
      "!installers/**/*"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-modal": "^3.16.0",
    "concurrently": "^7.3.0",
    "electron": "^20.1.0",
    "electron-builder": "^23.6.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^17.1.1",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.3.1",
    "wait-on": "^6.0.1",
    "webpack-cli": "^4.10.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "build": {
    "appId": "com.auto-tt-recorder-gui.app",
    "artifactName": "Auto-TT-Recorder-GUI-${version}-${os}-${arch}.${ext}",
    "compression": "normal",
    "asar": false,
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "luckytyphlosion",
        "repo": "auto-tt-recorder-gui"
      }
    ],
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "differentialPackage": true,
      "deleteAppDataOnUninstall": true
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**",
      "auto-tt-recorder_v*.*.*_for_gui/**",
      "data/**",
      "!build_scripts/**/*",
      "!node_modules/**/*",
      "!webpack/**/*"
    ]
  },
  "license": "GPL-2.0-or-later"
}
